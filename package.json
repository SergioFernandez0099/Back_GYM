{
  "name": "back-gym",
  "version": "1.0.0",
  "author": "Sergio Fernández Muñoz",
  "license": "MIT",
  "description": "Backend que proporciona una API para gestionar una aplicación de sesiones de entrenamiento.",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "// ========== DESARROLLO ==========": "",
    "build": "pnpm tsc",
    "dev": "pnpm tsc && node ./dist/src/server.js",
    "// ========== PRODUCCIÓN ==========": "",
    "start": "cross-env NODE_ENV=production node ./dist/src/server.js",
    "start:pm2": "pm2 start ecosystem.config.cjs --env production",
    "stop:pm2": "pm2 stop gym-api",
    "restart:pm2": "pm2 restart gym-api",
    "reload:pm2": "pm2 reload gym-api --update-env",
    "logs:pm2": "pm2 logs gym-api",
    "monit:pm2": "pm2 monit",
    "// ========== PRISMA ==========": "",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:status": "prisma migrate status",
    "db:migrate:reset": "prisma migrate reset",
    "db:seed": "prisma db seed",
    "db:reset": "pnpm run db:migrate:reset && pnpm run db:seed"
  },
  "keywords": [],
  "packageManager": "pnpm@10.28.2+sha512.41872f037ad22f7348e3b1debbaf7e867cfd448f2726d9cf74c08f19507c31d2c8e7a11525b983febc2df640b5438dee6023ebb1f84ed43cc2d654d2bc326264",
  "devDependencies": {
    "@types/node": "^25.0.6",
    "@types/pg": "^8.16.0",
    "dotenv": "^17.2.3",
    "prisma": "^7.2.0"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@types/express": "^5.0.6",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cross-env": "^10.1.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "path": "^0.12.7",
    "typescript": "^5.9.3",
    "url": "^0.11.4",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0"
  }
}
