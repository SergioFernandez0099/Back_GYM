{
  "compilerOptions": {
    "target": "ES2023",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": false,
    "resolveJsonModule": true,
    "allowJs": true,
    "outDir": "./dist",
    "ignoreDeprecations": "6.0"
  },
  "include": ["prisma/**/*", "src/**/*"],
  "exclude": ["node_modules", "dist"]
}